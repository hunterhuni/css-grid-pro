{
  "name": "css-grid-pro",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile-sass": "sass --watch --no-source-map src/main.scss src/styles.css",
    "build:compile": "sass --no-source-map src/main.scss build/compiled.css",
    "build:prefixes": "postcss build/compiled.css --use autoprefixer --output build/prefixed.css",
    "build:optimize": "csso build/prefixed.css --output build/styles.css"
  },
  "author": "Hunor Szatm√°ri",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "csso-cli": "^4.0.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "sass": "^1.69.3"
  }
}
